{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "AnalysisType": {
      "oneOf": [
        {
          "type": "string",
          "const": "lookup_table"
        }
      ]
    },
    "LookupName": {
      "type": "string"
    },
    "Enabled": {
      "type": "string"
    },
    "Filename": {
      "oneOf": [
        {
          "type": "string"
        },
        {
          "type": "object",
          "properties": {
            "RoleARN": {
              "type": "string"
            },
            "ObjectPath": {
              "type": "string"
            },
            "PeriodMinutes": {
              "type": "string"
            },
            "AlarmPeriodMinutes": {
              "type": "string"
            },
            "ObjectKMSKey": {
              "type": "string"
            }
          }
        }
      ]
    },
    "Schema": {
      "type": "string"
    },
    "LogTypeMap": {
      "type": "object",
      "properties": {
        "PrimaryKey": {
          "type": "string"
        },
        "AssociatedLogTypes": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "LogType": {
                "type": "string"
              },
              "Selectors": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              }
            }
          }
        }
      }
    },
    "Description": {
      "type": "string"
    },
    "Reference": {
      "type": "string"
    }
  },
  "required": [
    "AnalysisType",
    "LookupName",
    "Enabled",
    "Filename",
    "Schema",
    "LogTypeMap"
  ]
}